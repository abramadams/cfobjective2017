{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . --platform=win32 --arch=x64 --overwrite",
    "package-linux": "electron-packager . --platform=linux --overwrite",
    "package-mac": "electron-packager . --platform=darwin --overwrite"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "~2.0.4",
    "electron-packager": "^8.7.2"
  },
  "dependencies": {
    "mysql": "^2.15.0"
  }
}
